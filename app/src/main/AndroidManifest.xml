<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.josu.inmoprovider" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Principal"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Secundaria"
            android:label="@string/title_activity_secundaria" >
        </activity>
        <activity
            android:name=".PantallaCompleta"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
        </activity>
        <activity
            android:name=".Anadir"
            android:label="@string/title_activity_anadir" >
        </activity>
        <provider
            android:name=".Proveedor"
            android:authorities="com.example.josu.inmoprovider"
            android:enabled="true"
            android:exported="true" >
        </provider>
    </application>

</manifest>